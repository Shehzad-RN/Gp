{
  "expo": {
    "name": "Groupick",
    "slug": "Groupick",
    "version": "1.0.1",
    "orientation": "portrait",
    "assetBundlePatterns": ["**/*"],
    "icon": "./Assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.groupickk.app",
      "buildNumber": "2.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.groupickk.app",
      "versionCode": 2
    }
  },
  "plugins": [
    [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let you share them with your friends."
      }
    ]
  ],
  "name": "ecom"
}
